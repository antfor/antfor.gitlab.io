{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "postbuild": "purgecss --config ./purgecss.config.cjs --output ./dist/assets/",
    "lint": "eslint .",
    "preview": "vite preview",
    "profile": "lighthouse --view --chrome-flags='--headless'",
    "analyze": "vite-bundle-visualizer",
    "compress": "find dist -type f -regex '.*\\.\\(htm\\|html\\|txt\\|text\\|js\\|xml\\|json\\|mjs\\|svg\\|yaml\\|yml\\|toml\\|css\\)$' -exec brotli -f -k -Z {} \\; && ls -lh dist/*;"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.6",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.1",
    "mathjs": "^13.2.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "twgl.js": "^5.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^15.15.0",
    "purgecss": "^6.0.0",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^5.4.21"
  }
}
